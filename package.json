{
  "name": "voty",
  "engines": {
    "node": "=14 || =16.16 || =20"
  },
  "version": "1.3.0-development",
  "private": true,
  "scripts": {
    "dev": "echo 'Please run ./dev.sh'",
    "build": "next build",
    "start": "next start",
    "start:test": "next &",
    "cy": "dotenv -- cypress open",
    "test": "dotenv -- cypress run --record false",
    "deploy": "./ansible/scripts/deploy.py",
    "deploy:dev": "ansible-playbook -i ansible/dev ansible/site.yml --tags app",
    "deploy:prod": "ansible-playbook -i ansible/prod ansible/site.yml --tags app",
    "deploy:backup": "ansible-playbook -i ansible/backup ansible/site.yml --tags app",
    "site:dev": "ansible-playbook -i ansible/dev ansible/site.yml",
    "site:prod": "ansible-playbook -i ansible/prod ansible/site.yml",
    "site:backup": "ansible-playbook -i ansible/backup ansible/site.yml",
    "migrate:dev": "ansible-playbook -i ansible/dev ansible/migrate.yml",
    "migrate:prod": "ansible-playbook -i ansible/prod ansible/migrate.yml",
    "migrate:backup": "ansible-playbook -i ansible/backup ansible/migrate.yml",
    "init:db": "npx prisma migrate dev --preview-feature",
    "reset:db": "ts-node ansible/scripts/prisma-loader-cli.ts cypress/fixtures/testdb.yml",
    "load:db": "ts-node ansible/scripts/prisma-loader-cli.ts",
    "ci:test": "yarn ci:release && yarn init:db && yarn start:test && dotenv -- cypress run --record",
    "ci:release": "yarn install --frozen-lockfile && npx prisma generate && yarn run build",
    "graphql": "graphql-codegen --config codegen.yml",
    "check:ts": "tsc --noEmit",
    "nexus:reflection": "npx ts-node -T ./graphql/schema/index.ts --nexus-exit",
    "lint": "next lint",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@apollo/client": "^3.3.4",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/runtime": "^1.6.22",
    "@next/mdx": "^12.0.0",
    "@nexus/schema": "^0.19.2",
    "@prisma/client": "2.22.1",
    "@rebass/forms": "^4.0.6",
    "apollo-server-micro": "^2.19.0",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.4.1",
    "dayjs": "^1.9.7",
    "formidable": "^1.2.2",
    "formik": "^2.1.5",
    "graphql": "^15.4.0",
    "graphql-middleware": "^4.0.2",
    "graphql-request": "^3.3.0",
    "graphql-scalars": "^1.7.0",
    "graphql-shield": "^7.4.3",
    "imap-simple": "^5.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "marked": "^4.0.10",
    "mjml": "^4.6.3",
    "mustache": "^4.1.0",
    "next": "^12.1.0",
    "nexus-plugin-prisma": "0.26.0-next.1",
    "nexus-plugin-shield": "^0.2.0",
    "nodemailer": "6.6.1",
    "prismjs": "1.27.0",
    "qrcode.react": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable-list": "^4.0.3",
    "react-markdown": "^5.0.3",
    "react-minimal-pie-chart": "^8.0.1",
    "react-new-window": "^0.1.2",
    "react-player": "^2.6.0",
    "rebass": "^4.0.7",
    "recoil": "^0.3.1",
    "remark-gfm": "^1.0.0",
    "remark-images": "^2.0.0",
    "theme-ui": "0.3.1",
    "winston": "^3.3.3",
    "winston-mail": "^2.0.0",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.6",
    "@divyenduz/ts-graphql-plugin": "^0.1.0",
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-document-nodes": "1.17.9",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@testing-library/cypress": "^7.0.6",
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.4.0",
    "@types/formidable": "^1.0.31",
    "@types/imap-simple": "^4.2.4",
    "@types/lodash": "^4.14.161",
    "@types/mdx-js__react": "^1.5.2",
    "@types/mjml": "^4.0.4",
    "@types/mustache": "^4.1.0",
    "@types/node": "^14.14.11",
    "@types/nodemailer": "^6.4.1",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^17.0.0",
    "@types/rebass": "^4.0.6",
    "@types/rebass__forms": "^4.0.3",
    "@types/theme-ui": "^0.3.7",
    "@types/winston-mail": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-istanbul": "^6.0.0",
    "check-code-coverage": "^1.10.0",
    "concurrently": "^5.3.0",
    "cypress": "13.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "js-yaml": "^3.14.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "prisma": "2.22.1",
    "semantic-release": "^17.3.7",
    "simplytyped": "^3.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5",
    "yargs": "16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teachen-ch/voty.git"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs/CHANGELOG.md",
            "package.json"
          ]
        }
      ]
    ]
  },
  "resolutions": {
    "ssri": "^8.0.1",
    "is-svg": "^4.2.2",
    "ws": "^7.4.6",
    "set-value": "^4.0.1",
    "json-schema": "^0.4.0",
    "ansi-regex": "^5.0.1",
    "nth-check": "^2.0.1",
    "url-parse": "^1.5.2",
    "css-what": "^5.0.1",
    "glob-parent": "^5.1.2",
    "hosted-git-info": "^3.0.8",
    "trim": "^0.0.3",
    "marked": "^4.0.10",
    "@graphql-tools/git-loader": "^6.2.6",
    "node-fetch": "^2.6.1",
    "yargs-parser": "^13.1.2",
    "lodash": "^4.17.21",
    "mem": "^4.0.0",
    "cross-fetch": "^2.2.6",
    "eventsource": "^2.0.2",
    "json5": "^2.2.2",
    "fast-xml-parser": "^4.2.5",
    "loader-utils": "^2.0.4"
  }
}
