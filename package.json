{
  "name": "voty",
  "version": "1.1.1-development",
  "private": true,
  "scripts": {
    "dev": "yarn run next",
    "build": "next build",
    "start": "next start",
    "start:test": "yarn run next &",
    "cy": "dotenv -- cypress open",
    "test": "dotenv -- cypress run --record false",
    "deploy": "./ansible/scripts/deploy.py",
    "deploy:dev": "ansible-playbook -i ansible/dev ansible/site.yml --tags app",
    "deploy:prod": "ansible-playbook -i ansible/prod ansible/site.yml --tags app",
    "deploy:backup": "ansible-playbook -i ansible/backup ansible/site.yml --tags app",
    "site:dev": "ansible-playbook -i ansible/dev ansible/site.yml",
    "site:prod": "ansible-playbook -i ansible/prod ansible/site.yml",
    "site:backup": "ansible-playbook -i ansible/backup ansible/site.yml",
    "migrate:dev": "ansible-playbook -i ansible/dev ansible/migrate.yml",
    "migrate:prod": "ansible-playbook -i ansible/prod ansible/migrate.yml",
    "migrate:backup": "ansible-playbook -i ansible/backup ansible/migrate.yml",
    "init:db": "npx prisma migrate dev --preview-feature",
    "reset:db": "ts-node ansible/scripts/prisma-loader-cli.ts cypress/fixtures/testdb.yml",
    "ci:test": "yarn ci:release && yarn init:db && yarn start:test && dotenv -- cypress run --record",
    "ci:release": "yarn install --frozen-lockfile && npx prisma generate && yarn run build",
    "graphql": "graphql-codegen --config codegen.yml",
    "nexus:reflection": "npx ts-node -T ./graphql/schema/index.ts --nexus-exit",
    "lint": "npx eslint ./",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@apollo/client": "^3.3.4",
    "@mdx-js/loader": "^1.6.19",
    "@mdx-js/runtime": "^1.6.19",
    "@next/mdx": "^10.0.3",
    "@nexus/schema": "^0.19.2",
    "@prisma/client": "2.13.0",
    "@rebass/forms": "^4.0.6",
    "apollo-server-micro": "^2.19.0",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.4.1",
    "dayjs": "^1.9.7",
    "formidable": "^1.2.2",
    "formik": "^2.1.5",
    "graphql": "^15.4.0",
    "graphql-middleware": "^4.0.2",
    "graphql-request": "^3.3.0",
    "graphql-scalars": "^1.7.0",
    "graphql-shield": "^7.4.3",
    "imap-simple": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mjml": "^4.6.3",
    "mustache": "^4.1.0",
    "next": "10.0.3",
    "nexus-plugin-prisma": "0.26.0-next.1",
    "nexus-plugin-shield": "^0.2.0",
    "nodemailer": "^6.4.11",
    "qrcode.react": "^1.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-draggable-list": "^4.0.3",
    "react-markdown": "^5.0.3",
    "react-minimal-pie-chart": "^8.0.1",
    "react-new-window": "^0.1.2",
    "react-player": "^2.6.0",
    "rebass": "^4.0.7",
    "recoil": "^0.1.2",
    "remark-gfm": "^1.0.0",
    "remark-images": "^2.0.0",
    "theme-ui": "0.3.1",
    "winston": "^3.3.3",
    "winston-mail": "^2.0.0",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.8.5",
    "@divyenduz/ts-graphql-plugin": "^0.1.0",
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-document-nodes": "1.17.9",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@prisma/cli": "^2.13.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@testing-library/cypress": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.4.0",
    "@types/formidable": "^1.0.31",
    "@types/imap-simple": "^4.2.4",
    "@types/lodash": "^4.14.161",
    "@types/mdx-js__react": "^1.5.2",
    "@types/mjml": "^4.0.4",
    "@types/mustache": "^4.1.0",
    "@types/node": "^14.14.11",
    "@types/nodemailer": "^6.4.0",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^17.0.0",
    "@types/rebass": "^4.0.6",
    "@types/rebass__forms": "^4.0.3",
    "@types/theme-ui": "^0.3.7",
    "@types/winston-mail": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-istanbul": "^6.0.0",
    "check-code-coverage": "^1.10.0",
    "concurrently": "^5.3.0",
    "cypress": "6.3.0",
    "cypress-image-snapshot": "^4.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.2",
    "js-yaml": "^3.14.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "semantic-release": "^17.3.7",
    "simplytyped": "^3.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.5",
    "yargs": "^16.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teachen-ch/voty.git"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs/CHANGELOG.md",
            "package.json"
          ]
        }
      ]
    ]
  }
}
